# Application Settings
DEBUG=true
SECRET_KEY=your-development-secret-key-here

# Testing mode (enables fakeredis for tests)
TESTING=false

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT Configuration
JWT_SECRET_KEY=your-jwt-secret-key-for-development
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=10080

# Amazon SP-API Configuration
AMAZON_CLIENT_ID=your-dev-amazon-client-id
AMAZON_CLIENT_SECRET=your-dev-amazon-client-secret
AMAZON_REFRESH_TOKEN=your-dev-amazon-refresh-token

# AWS Configuration (LocalStack)
AWS_ACCESS_KEY_ID=test-access-key
AWS_SECRET_ACCESS_KEY=test-secret-key
AWS_REGION=us-east-1
AWS_ENDPOINT_URL=http://localhost:4566
ROLE_ARN=arn:aws:iam::123456789012:role/test-role

# SQS Configuration (LocalStack)
SQS_QUEUE_URL_ANY_OFFER=http://sqs.us-east-1.localhost.localstack.cloud:4566/000000000000/amazon-any-offer-changed-queue
SQS_QUEUE_URL_FEED_PROCESSING=http://sqs.us-east-1.localhost.localstack.cloud:4566/000000000000/feed-processing-queue

# Business Logic Settings
FEED_BATCH_SIZE=100
MAX_RETRIES=3

# Logging
LOG_LEVEL=DEBUG

CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0
